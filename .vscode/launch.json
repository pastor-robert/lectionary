{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app"
            ],
            "jinja": true
        },
        {
            "name": "Python : main:app",
            "cwd": "${workspaceFolder}",
            "type": "python",
            "request": "launch",
            "program": "venv/bin/uvicorn",
            "args": [
                "main:app",
                "--host=127.0.0.1",
                "--port=8000",
                "--reload",
                "--log-level=error"
            ],
            "console": "integratedTerminal",
            "postDebugTask": "killdebugger"
        }
    ]
}